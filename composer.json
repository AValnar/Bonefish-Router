{
    "name": "av/bonefish-router",
    "description": "Routing package for Bonefish",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6.0",
        "av/bonefish-injection": "dev-master",
        "symfony/http-foundation": "^2.7",
        "nikic/fast-route": "^0.6"
    },
    "require-dev": {
       "phpunit/phpunit": "^4.5"
    },
    "autoload": {
      "psr-4": {
        "Bonefish\\Router\\": ["src"],
        "Bonefish\\Tests\\Router\\": ["tests"]
      }
    }
}
